{
    "name": "vendor/package-name",
    "description": "Enter a description for your package here",
    "keywords": ["hydephp", "package", "extension"],
    "homepage": "https://github.com/vendor/package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "John Doe",
            "email": "hello@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
		"hyde/framework": "^1.0",
        "illuminate/support": "^10.0"
    },
	"require-dev": {
		"pestphp/pest": "^2.0"
	},
    "autoload": {
        "psr-4": {
            "Vendor\\PackageName\\": "src"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Vendor\\PackageName\\Tests\\": "tests"
		}
	},
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\PackageName\\PackageNameServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
